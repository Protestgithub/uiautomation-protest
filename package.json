{
  "name": "comviva_poc",
  "version": "1.0.0",
  "main": "index.js",
  "test:sanity": "npx cypress-tags run -e 'TAGS=@prerequisite'",
  "test": "cypress run --spec \"**/*.feature\"",
  "scripts": {
    "start": "node ./serve.js",
    "cy:open": "cypress open",
    "cy:report": "node cucumber-html-report.js",
    "cy:run": "cypress run",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:clear": "if exist allure-results rmdir /q /s allure-results && if exist allure-report rmdir /q /s allure-report && if exist cypress\\screenshots rmdir /q /s cypress\\screenshots && if exist cypress\\videos rmdir /q /s cypress\\videos",
    "pretest": "npm run allure:clear",
    "test": "npm run cy:run || npm run posttest",
    "posttest": "npm run allure:report",
    "merge": "mochawesome-merge cypress/reports/*.json > cypress/reports/output.json",
    "marge": "marge cypress/reports/output.json --reportDir ./ --inline",
    "cy:Prerequisite": "npx cypress-tags run -e 'TAGS=@prerequisite' --browser chrome",
    "cy:Test": "npx cypress-tags run -e 'TAGS=@test' --browser chrome",
    "cy:parallel": "cypress-parallel -s cy:run -t 1 -d 'cypress/e2e/BDD/CreationOfUsers/**/*.feature'",
    "cy:parallel:many1": "cypress-parallel -s cy:run -t 2 -d 'cypress/e2e/BDD/Tests/**/*.feature'",
    "cy:parallel:many": "cypress-parallel -s cy:run -t 2 -d 'cypress/e2e/BDD/PreRequisite/**/*.feature'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^12.1.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@shelex/cypress-allure-plugin": "^2.30.3",
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "cypress": "^10.6.0",
    "cypress-cucumber-attach-screenshots-to-failed-steps": "^1.0.0",
    "cypress-fail-fast": "^5.0.0",
    "cypress-failed-log": "^2.9.5",
    "cypress-file-upload": "^5.0.8",
    "cypress-iframe": "^1.0.1",
    "cypress-log-to-output": "^1.1.2",
    "cypress-recurse": "^1.20.0",
    "cypress-wait-until": "^1.7.2",
    "cypress-xpath": "^1.6.2",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.2",
    "mochawesome-merge": "^4.3.0",
    "multiple-cucumber-html-reporter": "^1.18.0",
    "typescript": "^4.4.2",
    "webpack": "^5.70.0"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/**/*.{js,ts}",
    "cucumberJson": {
      "generate": true
    }
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.6",
    "circular-json": "^0.5.9",
    "crypto-js": "^4.1.1",
    "cypress-parallel": "^0.13.0",
    "cypress-terminal-report": "^5.2.0",
    "dayjs": "^1.11.1",
    "flatted": "^3.2.5",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "oracledb": "^5.5.0",
    "pg": "^8.11.3",
    "pg-promise": "^11.5.4",
    "randexp": "^0.5.3",
    "regex-parser": "^2.2.11",
    "selenium-webdriver": "^4.1.1"
  },
  "description": ""
}
